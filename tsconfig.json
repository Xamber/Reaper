{
  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
    "moduleResolution": "node",
    "sourceMap": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "removeComments": false,
    "noImplicitAny": false,
    "declaration": false,
    "noLib": false,
    "listFiles": false,
    "outDir": "dest"
  },
  "typeRoots": [
    "../node_modules/@types"
  ],
  "exclude": [
    "node_modules"
  ],
  "lib": [
    "es6"
  ],
  "types": [
    "node",
    "core-js"
  ]
}